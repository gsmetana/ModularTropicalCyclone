          RowBox[{"D", "[", 
           RowBox[{
            RowBox[{"tMOIST", "[", "p", "]"}], ",", "p"}], "]"}]}], " ", "-", 
         " ", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"L", "*", "\[Sigma]", "*", 
            RowBox[{
             RowBox[{"h", "[", 
              RowBox[{"pSWITCH", "-", "p"}], "]"}], "/", 
